<%= form_for(@user, url:users_create_path) do |f| %>
<table>
    
<tr>
<th><%= f.label :name %></th>    
<td><%= f.text_field :name %></td>    
</tr>

<tr>
<th><%= f.label :username %></th>    
<td><%= f.text_field :username %></td>       
</tr>

<tr>
<th><%= f.label :location %></th>    
<td><%= f.text_field :location %></td>     
</tr>

<tr>
<th><%= f.label :about %></th>    
<td><%= f.text_field :about %></td>        
</tr>

<tr>
<th><%= f.submit %></th>    

</tr>
</table>
<% end %>